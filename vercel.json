{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  },
  "functions": {
    "src/app/api/groups/route.ts": {
      "maxDuration": 3000,
      "memory": 2048
    },
    "src/app/api/results/route.ts": {
      "maxDuration": 3000,
      "memory": 2048
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs"
}